# Frontend Environment Variables for Production

# API URL - Update this with your deployed backend URL
VITE_API_URL=https://your-backend-url.onrender.com/api

# App Configuration
VITE_APP_NAME=Campus Companion
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true

# External Services (add when needed)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
VITE_FIREBASE_CONFIG=your_firebase_config_here

# Debug Mode
VITE_DEBUG_MODE=false

# Environment
NODE_ENV=production
VITE_NODE_ENV=production